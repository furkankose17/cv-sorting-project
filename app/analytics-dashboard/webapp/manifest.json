{
    "_version": "1.49.0",
    "sap.app": {
        "id": "cv.sorting.analytics",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "analyticsService": {
                "uri": "/api/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "matchingService": {
                "uri": "/api/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "jouleService": {
                "uri": "/api/ai/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "analytics-display": {
                    "semanticObject": "Analytics",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            },
            "outbounds": {
                "displayCandidate": {
                    "semanticObject": "Candidate",
                    "action": "manage"
                },
                "displayCandidates": {
                    "semanticObject": "Candidate",
                    "action": "display"
                },
                "displayJob": {
                    "semanticObject": "JobPosting",
                    "action": "display"
                },
                "uploadCV": {
                    "semanticObject": "CVDocument",
                    "action": "upload"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://business-objects-experience"
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "rootView": {
            "viewName": "cv.sorting.analytics.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.f": {},
                "sap.suite.ui.microchart": {},
                "sap.viz": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cv.sorting.analytics.i18n.i18n"
                }
            },
            "": {
                "dataSource": "analyticsService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "matching": {
                "dataSource": "matchingService",
                "preload": true
            },
            "joule": {
                "dataSource": "jouleService",
                "preload": false
            },
            "dashboard": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "data": {
                        "pipelineData": null,
                        "skillsData": null,
                        "trendsData": null,
                        "isLoading": true
                    }
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "cv.sorting.analytics.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "name": "dashboard",
                    "pattern": "",
                    "target": "dashboard"
                },
                {
                    "name": "skills",
                    "pattern": "skills",
                    "target": "skills"
                },
                {
                    "name": "pipeline",
                    "pattern": "pipeline",
                    "target": "pipeline"
                },
                {
                    "name": "trends",
                    "pattern": "trends",
                    "target": "trends"
                },
                {
                    "name": "interviews",
                    "pattern": "interviews",
                    "target": "interviews"
                }
            ],
            "targets": {
                "dashboard": {
                    "viewName": "Dashboard",
                    "viewLevel": 1
                },
                "skills": {
                    "viewName": "SkillsAnalytics",
                    "viewLevel": 2
                },
                "pipeline": {
                    "viewName": "PipelineOverview",
                    "viewLevel": 2
                },
                "trends": {
                    "viewName": "Trends",
                    "viewLevel": 2
                },
                "interviews": {
                    "viewName": "InterviewAnalytics",
                    "viewLevel": 2
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "cv-sorting"
    }
}
