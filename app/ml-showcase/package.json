{
  "name": "ml-showcase",
  "version": "1.0.0",
  "description": "ML Service Monitor - Test and monitor ML service endpoints",
  "private": true,
  "sapux": true,
  "sapuxLayer": "CUSTOMER_BASE",
  "dependencies": {},
  "devDependencies": {
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^3.0.0"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#mlshowcase-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#mlshowcase-display\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist"
  }
}
