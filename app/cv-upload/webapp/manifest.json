{
    "_version": "1.49.0",
    "sap.app": {
        "id": "cv.sorting.cvupload",
        "type": "application",
        "title": "CV Upload",
        "description": "Upload and process CV documents",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "cvService": {
                "uri": "/api/candidates/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "candidateService": {
                "uri": "/api/candidates/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "cv-upload-display": {
                    "semanticObject": "CVDocument",
                    "action": "upload",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            },
            "outbounds": {
                "displayCandidate": {
                    "semanticObject": "Candidate",
                    "action": "manage"
                },
                "displayAnalytics": {
                    "semanticObject": "Analytics",
                    "action": "display"
                },
                "displayJob": {
                    "semanticObject": "JobPosting",
                    "action": "display"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://document"
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "rootView": {
            "viewName": "cv.sorting.cvupload.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.ui.unified": {},
                "sap.f": {},
                "sap.uxap": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "": {
                "dataSource": "cvService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "candidate": {
                "dataSource": "candidateService",
                "preload": true
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cv.sorting.cvupload.i18n.i18n"
                }
            },
            "upload": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "data": {
                        "files": [],
                        "uploadProgress": 0,
                        "isProcessing": false
                    }
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "cv.sorting.cvupload.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "name": "upload",
                    "pattern": "",
                    "target": "upload"
                },
                {
                    "name": "documents",
                    "pattern": "documents",
                    "target": "documents"
                },
                {
                    "name": "documentDetail",
                    "pattern": "documents/{documentId}",
                    "target": "documentDetail"
                }
            ],
            "targets": {
                "upload": {
                    "viewName": "Upload",
                    "viewLevel": 1
                },
                "documents": {
                    "viewName": "Documents",
                    "viewLevel": 1
                },
                "documentDetail": {
                    "viewName": "DocumentDetail",
                    "viewLevel": 2
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "cv-sorting"
    }
}
